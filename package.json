{
  "name": "jalp",
  "version": "1.0.0",
  "description": "Just Another Learning Platform (JALP)",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "ci:client": "cd ./client && npm ci ",
    "ci:server": "cd ./server && npm ci ",
    "ci": "npm run ci:server && npm run ci:client",
    "build:client": "cd ./client && npm run build",
    "build:server": "cd ./server && npm run build",
    "build": "npm run ci && npm run build:server && npm run build:client",
    "codegen": "cd ./server && npm run codegen",
    "deps:client": "cd ./client && npm run deps",
    "deps:server": "cd ./server && npm run deps",
    "deps": "npm run deps:server && npm run deps:client",
    "start:client": "cd ./client && npm run start",
    "start:server": "cd ./server && npm run start --"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tscz/jalp.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tscz/jalp/issues"
  },
  "homepage": "https://github.com/tscz/jalp#readme"
}